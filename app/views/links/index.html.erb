<p style="color: green"><%= notice %></p>

<h1>Links</h1>
<%= render partial: 'links/go_back', locals: { go_back_url: root_path } %>
<div id="private_links">
<% @links.each do |link| %>
  <%= render partial: 'links/link', locals: { link: link, is_index_view: true } %>
<% end %>
</div>

<%= paginate @links %>
