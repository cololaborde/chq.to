<p style="color: green"><%= notice %></p>

<h1>Private links</h1>
<%= link_to "New private link", new_private_link_path %>

<div id="private_links">
  <% @private_links.each do |private_link| %>
    <%= render partial: 'links/link', locals: { link: private_link } %>
  <% end %>
</div>

<%= paginate @private_links %>
